{
  "name": "parcel-bundling",
  "version": "1.0.0",
  "description": "Create Hello world react app with parcel bundling",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && parcel ./src/index.html",
    "build:prod": "rimraf dist && parcel build ./src/index.html",
    "start": "rimraf dist && cross-env NODE_ENV=development parcel ./src/index.html --open",
    "start:prod": "rimraf dist && cross-env NODE_ENV=production parcel ./src/index.html --open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rodorro/parcel-bundling.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rodorro/parcel-bundling/issues"
  },
  "homepage": "https://github.com/rodorro/parcel-bundling#readme",
  "dependencies": {
    "@types/node": "^12.12.16",
    "cross-env": "^6.0.3",
    "dotenv": "latest",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.0",
    "sass": "^1.23.7",
    "typescript": "^3.7.3"
  }
}
